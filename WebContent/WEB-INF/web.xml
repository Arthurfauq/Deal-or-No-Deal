<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Deal or No Deal</display-name>
  <servlet>
    <servlet-name>GamePage</servlet-name>
    <servlet-class>myPackage.GamePage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GamePage</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BankOffer</servlet-name>
    <servlet-class>myPackage.BankOffer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BankOffer</servlet-name>
    <url-pattern>/BankOffer</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/Error404.jsp</location>
  </error-page>
</web-app>